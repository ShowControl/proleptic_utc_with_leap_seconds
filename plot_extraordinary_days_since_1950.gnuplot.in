set style data steps
set grid xtics
set grid ytics
set xtics rotate by 90
set xtics right
set title "Values of DATI Since 1950"
set xlabel "Date"
set ylabel "DTAI"
set xrange [2433283:2470000]
set yrange [-5:50]
load "@builddir@/set_xtics_005yr.gnuplot"
set nokey
set xtic rotate by 45
set terminal pdfcairo size 5,3
set output "values_of_DTAI_since_1950.pdf"
plot 'gnuplot.dat' using 1:2


